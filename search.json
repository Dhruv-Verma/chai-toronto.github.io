---
layout: null
---
[
{% for pub in site.publications %}
  {
    "id": "{{ pub.slug }}",
    "title": "{{ pub.title }}",
    "authors": "[{{ pub.authors  | join: ', '}}]",
    "venue": "{{ pub.venue }}", 
    "date": "{{ pub.date }}",
    "link": "{{ pub.link }}",
    "thumbnail": "{{ pub.thumbnail }}",
    "award": "{{ pub.award }}",
    "citation": "{{ pub.citation }}",
    "bibtex": "{{ pub.bibtex }}",
    "news": "{{ pub.news }}",
    "video": "{{ pub.video }}",
    "video_embed": "{{ pub.video }}"
  }{% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]